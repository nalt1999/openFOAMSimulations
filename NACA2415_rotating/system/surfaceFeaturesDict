FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      surfaceFeaturesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
AMI
{

	surfaces ("AMI.stl");


	includedAngle 150;

	subsetFeatures
	{

		nonManifoldEdges no;
	
		openEdges yes;
	
	}
}

NACA2415_openfoam
{

	surfaces ("NACA2415_openfoam.stl");


	includedAngle 150;

	subsetFeatures
	{

		nonManifoldEdges no;
	
		openEdges yes;
	
	}
}
